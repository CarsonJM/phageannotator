nextflow_process {

    name "Test Process: VIROMEQC_INSTALL"
    script "../main.nf"
    process "VIROMEQC_INSTALL"
    tag "modules"
    tag "modules_local"
    tag "viromeqc/install"


    test("No input required") {
        then {
            assertAll (
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }
}
